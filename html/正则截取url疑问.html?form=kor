	<script type="text/javascript">
		    function getvl(name) {
      			var reg = new RegExp("(^|\\?|&)" + name + "=([^&]*)(\\s|&|$)", "i");
      			if (reg.test(location.href)) return unescape(RegExp.$2.replace(/\+/g," "));
     			 return "";
    			};
    			
    			var aaa = getvl("bb");
    			console.log(aaa);
	</script>
